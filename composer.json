{
    "name": "shipstream/ups-rest-php-sdk",
    "description": "PHP SDK for UPS REST API",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Shipstream",
            "email": "info@shipstream.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "jane-php/open-api-runtime": "^7.6",
        "php-http/curl-client": "^2.3",
        "symfony/polyfill-php81": "^1.28"
    },
    "require-dev": {
        "jane-php/open-api-3": "^7.6",
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "ShipStream\\Ups\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.0"
        },
        "sort-packages": true
    }
}
